#!/usr/bin/python

######
# Modify a property of a player

import sys

from Player import Player
from Alignment import Alignment
from Database import DB

p = Player.load_by_name(sys.argv[1])
if p == None:
    print "Player not found"
    sys.exit(1)
    
p.set_prop(sys.argv[2], sys.argv[3])
p.save()

DB.commit()
