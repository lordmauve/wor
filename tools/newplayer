#!/usr/bin/python

######
# Create a new player

from Player import Player
from Item import Item
from Alignment import Alignment
from Database import DB
from ItemLib.Rock import Rock

p = Player('darksatanic', 'wor', Alignment.WOOD)
rock = Rock()
p.inventory.add(rock)

Player.save_cache()
Item.save_cache()

DB.commit()
